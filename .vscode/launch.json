{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "(gdb) Launch",
         "type": "cppdbg",
         "request": "launch",
         "program": "${workspaceFolder}/build/gem_castle.app",
         "args": [],
         "stopAtEntry": false,
         "cwd": "${workspaceFolder}/build",
         "environment": [],
         "externalConsole": false,
         "MIMode": "gdb",
         "setupCommands": [
            {
               "description": "Enable pretty-printing for gdb",
               "text": "-enable-pretty-printing",
               "ignoreFailures": true
            }
         ],
         "logging": {
            "moduleLoad": false,
            "trace": true
         },
         "preLaunchTask": "${defaultBuildTask}"
      }
   ]
}